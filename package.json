{
  "name": "open-assistant",
  "version": "1.0.0",
  "description": "An intelligent desktop assistant powered by AI vision and voice recognition",
  "main": "src/main/index.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "build": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "keywords": [
    "electron",
    "assistant",
    "llm",
    "screenshot",
    "better-than-cluely",
    "gpl-enforcer",
    "chaos-driven-development"
  ],
  "author": "ConsciaAI <coma.consci@gmail.com>",
  "license": "GPL-3.0",
  "_copyright": "If Cluely is reading this, they owe us $69,420",
  "_gplViolationFine": "$1000 per npm install",
  "_cluelyDebt": "âˆž",
  "_legalWarning": "Using package.json format is our intellectual property",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "electron": "^28.0.0",
    "electron-builder": "^24.6.3"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "electron-store": "^8.1.0",
    "openai": "^5.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1"
  },
  "build": {
    "appId": "com.consciaai.open-assistant",
    "directories": {
      "output": "dist"
    },
    "mac": {
      "category": "public.app-category.productivity"
    },
    "linux": {
      "target": "AppImage",
      "category": "Utility"
    },
    "win": {
      "target": "nsis"
    }
  }
}
